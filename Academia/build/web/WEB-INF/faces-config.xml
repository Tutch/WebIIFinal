<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
<managed-bean>
        <managed-bean-name>AuthenticateBean</managed-bean-name>
        <managed-bean-class>managedBeans.AuthenticateBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
        <managed-bean-name>ListarAlunosBean</managed-bean-name>
        <managed-bean-class>managedBeans.ListarAlunosBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
        <managed-bean-name>FichaBean</managed-bean-name>
        <managed-bean-class>managedBeans.FichaBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
        <managed-bean-name>AssinalarBean</managed-bean-name>
        <managed-bean-class>managedBeans.AssinalarBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
        <managed-bean-name>ExerciciosBean</managed-bean-name>
        <managed-bean-class>managedBeans.ExerciciosBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
        <managed-bean-name>CadastrarProfessorBean</managed-bean-name>
        <managed-bean-class>managedBeans.CadastrarProfessorBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
        <managed-bean-name>AceitarAlunoBean</managed-bean-name>
        <managed-bean-class>managedBeans.AceitarAlunoBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
        <managed-bean-name>AlterarProfessorBean</managed-bean-name>
        <managed-bean-class>managedBeans.AlterarProfessorBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
        <managed-bean-name>AlterarAlunoBean</managed-bean-name>
        <managed-bean-class>managedBeans.AlterarAlunoBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
        <managed-bean-name>RelatorioGeralBean</managed-bean-name>
        <managed-bean-class>managedBeans.RelatorioGeralBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
</managed-bean>


<navigation-rule>
    <from-view-id>/professorAlterarDados.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>sucesso</from-outcome>
        <to-view-id>sucessoCadastroProfessor.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>falhou</from-outcome>
        <to-view-id>erroCadastroProfessor.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
<navigation-rule>
    <from-view-id>/alunoAlterarDados.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>sucesso</from-outcome>
        <to-view-id>sucessoCadastroProfessor.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>falhou</from-outcome>
        <to-view-id>erroCadastroProfessor.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
<navigation-rule>
    <from-view-id>/alunosPendentes.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>alunoAceito</from-outcome>
        <to-view-id>alunoAceito.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>erroAlunoAceito</from-outcome>
        <to-view-id>erroAlunoAceito.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>

<navigation-rule>
    <from-view-id>/index.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>Authenticated</from-outcome>
        <to-view-id>/home.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>AuthenticatedProfessor</from-outcome>
        <to-view-id>/homeProfessor.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>index</from-outcome>
        <to-view-id>../index.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>Failed</from-outcome>
        <to-view-id>erroLogin.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>CadastrarExercicio</from-outcome>
        <to-view-id>cadastrarExercicio.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>ExercicioCadastrado</from-outcome>
        <to-view-id>../home.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
        <from-outcome>caracteresInvalidos</from-outcome>
        <to-view-id>../caracteresInvalidos.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>

<navigation-rule>
    <from-view-id>/cadastroProfessor.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>professorCadastrado</from-outcome>
        <to-view-id>sucessoCadastroProfessor.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>erro</from-outcome>
        <to-view-id>erroCadastroProfessor.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>

<navigation-rule>
    <from-view-id>/alunoVerFicha.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>index</from-outcome>
        <to-view-id>index.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
</faces-config>
